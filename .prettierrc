{
	"semi": true,
	"useTabs": true,
	"printWidth": 100,
	"singleQuote": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
